<!-- res/drawable/rounded_corners.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="@android:color/white"/> <!-- Nền trắng -->
    <corners android:radius="20dp"/> <!-- Bo góc với bán kính 8dp -->
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"/>
    <stroke
        android:width="0dp"
        android:color="@android:color/transparent"/> <!-- Không có viền -->
</shape>
